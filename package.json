{
  "name": "gtomy-lib",
  "version": "0.0.6",
  "description": "Shared lib for react projects",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && rollup -c --bundleConfigAsCjs",
    "build:watch": "rimraf dist && npx rollup -c -w --bundleConfigAsCjs --watch.buildDelay 500",
    "prepare": "husky install",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint:check": "eslint ./src",
    "lint:fix": "eslint ./src --fix"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/GTomy/gtomy-lib.git"
  },
  "author": "GTomy",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/GTomy/gtomy-lib/issues"
  },
  "homepage": "https://gitlab.com/GTomy/gtomy-lib#readme",
  "dependencies": {
    "@heroicons/react": "^2.0.14",
    "autoprefixer": "^10.4.13",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "zustand": "^4.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/react": "^18.0.27",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "autoprefixer": "^10.4.13",
    "cssnano": "^5.1.14",
    "daisyui": "^2.47.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "postcss-js": "^4.0.0",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.39.4",
