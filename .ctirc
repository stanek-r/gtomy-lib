{
  "spinnerStream": "stdout",
  "progressStream": "stdout",
  "reasonerStream": "stderr",
  "options": [
    {
      "mode": "bundle",
      "project": "tsconfig.json",
      "exportFilename": "index.ts",
      "useSemicolon": true,
      "useBanner": false,
      "useTimestamp": false,
      "quote": "'",
      "directive": "",
      "fileExt": "none",
      "overwrite": true,
      "backup": false,
      "generationStyle": "auto",
      "include": [
        "src/**/*.*"
      ],
      "exclude": [
        "src/**/*.stories.*",
        "src/**/*.core.*",
        "src/locales/**"
      ],
      "skipEmptyDir": true,
      "startFrom": "./src",
      "output": "./src",
      "removeBackup": false,
      "forceYes": false
    }
  ]
}
